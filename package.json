{
  "name": "wooliesx",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node -r ts-node/register ./src/app.ts",
    "start:watch": "nodemon",
    "test": "jest",
    "test:coverage": "jest --coverage --verbose",
    "build": "rimraf ./build && tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ashleydass/wooliesx.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ashleydass/wooliesx/issues"
  },
  "homepage": "https://github.com/ashleydass/wooliesx#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "express-pino-logger": "^4.0.0",
    "lodash": "^4.17.15",
    "pino": "^5.16.0",
    "supertest": "^4.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/express-pino-logger": "^4.0.2",
    "@types/jest": "^25.1.2",
    "@types/lodash": "^4.14.149",
    "@types/pino": "^5.15.5",
    "@types/supertest": "^2.0.8",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.2.0",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePathIgnorePatterns": [
      "dist"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}
